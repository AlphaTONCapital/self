{
  "name": "@atoncap/agent-service-framework",
  "version": "0.1.0",
  "description": "Infrastructure for AI agents to discover, collaborate, and transact on TON/Telegram",
  "keywords": ["ai", "agents", "ton", "telegram", "blockchain", "services"],
  "homepage": "https://github.com/ATONCAP/agent-service-framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/ATONCAP/agent-service-framework.git"
  },
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "deploy:contracts": "cd contracts && pnpm deploy",
    "start:marketplace": "cd apps/marketplace && pnpm start",
    "cli": "node tools/cli.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "prettier": "^3.0.0",
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "inquirer": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
